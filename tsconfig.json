{
  "compilerOptions": {
    "target": "es2017",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "noEmitHelpers": false,
    "strictNullChecks": false,
    "baseUrl": "./*.ts",
    "outDir": "./build",
    "paths": {},
    "lib": [
      "es2017"
    ],
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}